import random

mystere = random.randint(0,100)
value = -1
vies = 7

while(mystere != value and vies != 0):
    value = int(input("Quel est votre nombre ?\n"))
    vies = vies - 1
    vies = 6

while(vies != value and value != 0):
    rand = random.randint(0,100)
    print("Cest gagn√© !\n")
    rand = 0
